    "@types/chrome": "^0.0.179",
    "@types/jest": "^26.0.21",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^6.0.0",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
